{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": 0,
        "no-multi-str": 0,
        "prefer-rest-params": 0,
        "no-param-reassign": 0,
        "import/prefer-default-export": 0,
        "consistent-return": 0,
        "no-restricted-syntax": 0,
        "no-shadow": 0,
        "guard-for-in": 0,
        "prefer-destructuring": 0
    },
    "ignorePatterns" : [
        "docs/*"
    ]
}
